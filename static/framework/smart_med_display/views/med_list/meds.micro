<table class='medtable'>
{%  for (var i = 0; i < meds.length; i++)  { 
         var m = meds[i];
%}
<tr {%= (i === 0) ? "class='selected'" : "" %}>
  <td>{%= (i === 0) ? ">" : ""%}</td>
  <td>{%= m.properName() %}</td>
  <td>{%= m.dose %}</td>
  <td>{%= m.unit %}</td>
  <td>{%= m.route %}</td>
  <td>{%= m.frequency %}</td>
  <td>{%= m.notes %}</td>
</tr>
{% } %}
</table>

<button id='DestroyButton'>Destroy</button>
