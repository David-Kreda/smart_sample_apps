<table class='medtable'>
<tr>
<th>SNOMED</th>
<th>Problem</th>
<th>Notes</th>
<th>Onset</th>
<th>Resolution</th>
<th></th>
</tr>
<%  for (var i = 0; i < problems.length; i++)  { 
         var problem = problems[i];
%>
<tr class='problem' <%=problem %>>
  <td> <%= problem.concept.substr(problem.concept.lastIndexOf("/")+1) %></td>
  <td><%= problem.title %></td>
  <td><%= problem.notes%></td>
  <td><%= problem.onset %></td>
  <td><%= problem.resolution %></td>
  <td>[<a href="#" class='delete_problem'>X</a>]</td>
</tr>
<% } %>
</table>
