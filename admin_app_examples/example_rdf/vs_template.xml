<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:sp="http://smartplatforms.org/terms#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:v="http://www.w3.org/2006/vcard/ns#">
 <sp:VitalSigns>
    <dc:date>{{DATE}}</dc:date>
    <sp:height>
      <sp:ValueAndUnit>
        <sp:value>1.02</sp:value>
        <sp:unit>m</sp:unit>
      </sp:ValueAndUnit>
    </sp:height>
    <sp:bloodPressure>
      <sp:BloodPressure>
        <sp:systolic>
          <sp:ValueAndUnit>
            <sp:value>{{SBP}}</sp:value>
            <sp:unit>mm[Hg]</sp:unit>
          </sp:ValueAndUnit>
        </sp:systolic>
        <sp:diastolic>
          <sp:ValueAndUnit>
            <sp:value>{{DBP}}</sp:value>
            <sp:unit>mm[Hg]</sp:unit>
          </sp:ValueAndUnit>
        </sp:diastolic>
        <sp:bodyPosition>
          <sp:CodedValue>
            <sp:code rdf:resource="http://smartplatforms.org/terms/code/bodyPosition#sitting"/>
            <dcterms:title>Sitting</dcterms:title>
          </sp:CodedValue>
        </sp:bodyPosition>
      </sp:BloodPressure>
    </sp:bloodPressure>
 </sp:VitalSigns>
</rdf:RDF>
